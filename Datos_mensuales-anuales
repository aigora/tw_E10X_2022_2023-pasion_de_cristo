	//el mes va de 0 a 12, siendo el 0 enero, hasta el 11 diciembre, y el 12 la media anual
	// el aï¿½o empezara en el 90 siendo el 0, y acabara en el 23
void Producible hidraulico(int mes,int ano,float Vector1[],Vector2[],Vector3[]){
int ano2;
if(ano>=90)
{
	ano2=ano-90;
}
else
{
	ano=ano+10;
}
int posicion;
char palabra[3];
switch(mes){
	case 0:
		palabra={e,n,e};
		break;
	case 1:
		palabra={f,e,b};
		break;
	case 2:
		palabra={m,a,r};
		break;
	case 3:
		palabra={a,b,r};
		break;
	case 4:
		palabra={m,a,y};
		break;
	case 5:
		palabra={j,u,n};
		break;
	case 6:
		palabra={j,u,l};
		break;
	case 7:
		palabra={a,g,o};
		break;
	case 8:
		palabra={s,e,p};
		break;
	case 9:
		palabra={o,c,t};
		break;
	case 10:
		palabra={n,o,v};
		break;
	case 11:
		palabra={d,i,c};
		break;
	case 12:
		palabra={a,n,o};
		break;
}
posicion= (ano2*13)+mes;
printf("En %c%c%c-%i, el Producible Hidraulico fue de %f GWh, el Indice producible fue de %f, y la probabilidad de ser superado fue del %f %.",palabra[0],palabra[1],palabra[2],Vector1[posicion],Vector2[posicion],Vector3[posicion]);
}
